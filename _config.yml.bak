theme: jekyll-theme-cayman
title: "BigData Analytics Pro - Santa Catarina"
description: "Plataforma avançada de análise de dados governamentais de SC com Machine Learning, APIs em tempo real e visualizações profissionais."
baseurl: "/topicos-bigdata-python"
url: "https://cordeirotelecom.github.io"

# Site settings
author: "BigData Analytics Team"
email: "contato@bigdataanalytics.com"

# Build settings
markdown: kramdown
highlighter: rouge

# Plugins
plugins:
  - jekyll-feed
  - jekyll-sitemap
  - jekyll-seo-tag

# GitHub Pages configuration
source: "."
destination: "_site"

# Include all necessary files
include:
  - "site_analise_dados"
  - "js"
  - "css"
  - "assets"
  - "aulas"
  - "datasets"
  - "materiais"
  - "docs"

# Exclude unnecessary files
exclude:
  - "README.md"
  - "Gemfile"
  - "Gemfile.lock"
  - "node_modules"
  - "vendor"
  - ".sass-cache"
  - ".jekyll-cache"
  - ".jekyll-metadata"

# Navigation
navigation:
  - title: "Plataforma"
    url: "/site_analise_dados/"
  - title: "Cronograma"
    url: "/cronograma"
  - title: "Materiais"
    url: "/materiais"
  - title: "Projetos"
    url: "/projetos"
  - title: "Contato"
    url: "/contato"

# Course information
course:
  name: "Tópicos de Big Data em Python"
  code: "BigData Analytics Pro"
  semester: "2024/1"
  credits: "Plataforma Completa"
  schedule: "24/7 Online"
